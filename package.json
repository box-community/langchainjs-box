{
  "name": "langchainjs-box",
  "version": "0.1.0",
  "description": "An integration package connecting Box and LangChain.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepublishOnly": "npm run build",
    "pack:dry": "npm pack --dry-run"
  },
  "keywords": [
    "langchain",
    "box",
    "document-loader",
    "typescript",
    "javascript"
  ],
  "author": "Box Developer Relations",
  "license": "MIT",
  "dependencies": {
    "box-node-sdk": "^10.0.0"
  },
  "devDependencies": {
    "@langchain/core": "0.3.78",
    "@types/node": "^20.19.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.78 <1.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/box-community/langchainjs-box.git"
  },
  "bugs": {
    "url": "https://github.com/box-community/langchainjs-box/issues"
  },
  "homepage": "https://github.com/box-community/langchainjs-box#readme"
}
